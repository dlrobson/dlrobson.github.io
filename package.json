    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/node": "^24.10.1",
    "cspell-lib": "^9.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^16.5.0",
    "prettier": "^3.5.0",
    "prettier-plugin-svelte": "^3.5.0",
    "svelte": "^5.53.1",
    "svelte-check": "^4.4.3",
    "svelte-fa": "^4.0.4",
